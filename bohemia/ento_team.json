{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "AllowStatement1",
			"Action": [
				"s3:ListAllMyBuckets",
				"s3:GetBucketLocation"
			],
			"Effect": "Allow",
			"Resource": [
				"arn:aws:s3:::*"
			]
		},
		{
			"Sid": "AllowStatement2A",
			"Action": [
				"s3:ListBucket"
			],
			"Effect": "Allow",
			"Resource": [
				"arn:aws:s3:::databrew-testing-bohemia"
			],
			"Condition": {
				"StringEquals": {
					"s3:prefix": [
						"",
						"bohemia-ento"
					]
				}
			}
		},
		{
			"Sid": "AllowStatement3",
			"Action": [
				"s3:ListBucket"
			],
			"Effect": "Allow",
			"Resource": [
				"arn:aws:s3:::databrew-testing-bohemia"
			],
			"Condition": {
				"StringLike": {
					"s3:prefix": [
						"bohemia-ento/*"
					]
				}
			}
		},
		{
			"Sid": "AllowStatement4A",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:DeleteObject",
				"s3:PutObject"
			],
			"Resource": [
				"arn:aws:s3:::databrew-testing-bohemia/bohemia-ento/*"
			]
		}
	]
}